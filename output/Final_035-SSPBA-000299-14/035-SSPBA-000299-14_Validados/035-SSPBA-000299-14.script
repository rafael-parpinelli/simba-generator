CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE CACHED TABLE SISTEMA(VERSAO VARCHAR(10) NOT NULL)
CREATE CACHED TABLE ATENDIMENTO(CODIGO_CHAVE_ATENDIMENTO BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NUMERO_CASO VARCHAR(30),CNPJ VARCHAR(14),NUMERO_BANCO NUMERIC(3),NOME_BANCO VARCHAR(200),NOME_BANCO_ATENDIMENTO VARCHAR(200),DATA_GERACAO_ARQUIVO DATE,NOME_FUNC_RESPONSAVEL VARCHAR(80),TELEFONE_FUNC_RESPONSAVEL VARCHAR(20),EMAIL_FUNC_RESPONSAVEL VARCHAR(100),DATA_INICIO_AFASTAMENTO DATE,DATA_FIM_AFASTAMENTO DATE,NUMERO_CORREIO_BACEN VARCHAR(20),NUMERO_OFICIO VARCHAR(20),DATA_OFICIO DATE,NUMERO_PROCESSO VARCHAR(20),NOME_TRIBUNAL VARCHAR(80),NUMERO_VARA VARCHAR(10),NOME_MAGISTRADO VARCHAR(80),CARGO_MAGISTRADO VARCHAR(80),DESCRICAO_CONTEUDO VARCHAR(500),HASH_ARQUIVO_1 VARCHAR(32),HASH_ARQUIVO_2 VARCHAR(32),HASH_ARQUIVO_3 VARCHAR(32),HASH_ARQUIVO_4 VARCHAR(32),HASH_ARQUIVO_5 VARCHAR(32),LINHAS_ARQUIVO_1 INTEGER,LINHAS_ARQUIVO_2 INTEGER,LINHAS_ARQUIVO_3 INTEGER,LINHAS_ARQUIVO_4 INTEGER,LINHAS_ARQUIVO_5 INTEGER,HASH_ARQUIVO_GAB109 VARCHAR(32),HASH_ARQUIVO_GAB112 VARCHAR(32),HASH_ARQUIVO_GAB800 VARCHAR(32),LINHAS_ARQUIVO_GAB109 INTEGER,LINHAS_ARQUIVO_GAB112 INTEGER,LINHAS_ARQUIVO_GAB800 INTEGER,VERSAO_VALIDADOR VARCHAR(10))
CREATE CACHED TABLE INVESTIGADO(CODIGO_INVESTIGADO BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,TIPO_PESSOA NUMERIC(1) NOT NULL,CPF_CNPJ_PESSOA VARCHAR(14) NOT NULL,NOME_PESSOA VARCHAR(80) NOT NULL,TEVE_RELACIONAMENTO INTEGER DEFAULT 0,TEVE_CONTA INTEGER DEFAULT 0,TEVE_BDV INTEGER DEFAULT 0,OBSERVACAO VARCHAR(500),DATA_INICIO_AFASTAMENTO DATE,DATA_FIM_AFASTAMENTO DATE)
CREATE CACHED TABLE AGENCIA(CODIGO_CHAVE_AGENCIA BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NUMERO_BANCO NUMERIC(3),NUMERO_AGENCIA NUMERIC(4),NOME_AGENCIA VARCHAR(50),ENDERECO_LOGRADOURO VARCHAR(80),ENDERECO_CIDADE VARCHAR(40),ENDERECO_UF VARCHAR(2),ENDERECO_PAIS VARCHAR(40),ENDERECO_CEP VARCHAR(8),TELEFONE_AGENCIA VARCHAR(20),DATA_ABERTURA_AGENCIA DATE,DATA_FECHAMENTO_AGENCIA DATE,LINHA_ARQUIVO INTEGER NOT NULL)
CREATE INDEX IDX_AGENCIA ON AGENCIA(NUMERO_BANCO,NUMERO_AGENCIA)
CREATE CACHED TABLE CONTA(CODIGO_CHAVE_CONTA BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,CODIGO_CHAVE_AGENCIA BIGINT,NUMERO_CONTA VARCHAR(20),TIPO_CONTA NUMERIC(1),DATA_ABERTURA_CONTA DATE,DATA_ENCERRAMENTO_CONTA DATE,MOVIMENTACAO_CONTA NUMERIC(1),LINHA_ARQUIVO INTEGER NOT NULL,CONSTRAINT SYS_FK_53 FOREIGN KEY(CODIGO_CHAVE_AGENCIA) REFERENCES AGENCIA(CODIGO_CHAVE_AGENCIA) ON DELETE CASCADE ON UPDATE CASCADE)
CREATE INDEX IDX_CONTA ON CONTA(CODIGO_CHAVE_AGENCIA,NUMERO_CONTA,TIPO_CONTA)
CREATE CACHED TABLE EXTRATO(CODIGO_CHAVE_EXTRATO BIGINT NOT NULL PRIMARY KEY,CODIGO_CHAVE_CONTA BIGINT NOT NULL,DATA_LANCAMENTO DATE,NUMERO_DOCUMENTO VARCHAR(20),DESCRICAO_LANCAMENTO VARCHAR(50),TIPO_LANCAMENTO NUMERIC(3),VALOR_LANCAMENTO NUMERIC(14),NATUREZA_LANCAMENTO VARCHAR(1),VALOR_SALDO NUMERIC(14),NATUREZA_SALDO VARCHAR(1),LOCAL_TRANSACAO VARCHAR(80),LINHA_ARQUIVO INTEGER NOT NULL,CONSTRAINT SYS_FK_58 FOREIGN KEY(CODIGO_CHAVE_CONTA) REFERENCES CONTA(CODIGO_CHAVE_CONTA) ON DELETE CASCADE ON UPDATE CASCADE)
CREATE CACHED TABLE PESSOA(CODIGO_CHAVE_PESSOA BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,CODIGO_INVESTIGADO BIGINT,TIPO_PESSOA NUMERIC(1),CPF_CNPJ_PESSOA VARCHAR(14),NOME_PESSOA VARCHAR(80),NOME_DOC_IDENTIFICACAO VARCHAR(50),NUMERO_DOC_IDENTIFICACAO VARCHAR(20),ENDERECO_LOGRADOURO VARCHAR(80),ENDERECO_CIDADE VARCHAR(40),ENDERECO_UF VARCHAR(2),ENDERECO_PAIS VARCHAR(40),ENDERECO_CEP VARCHAR(8),TELEFONE_PESSOA VARCHAR(20),VALOR_RENDA BIGINT,DATA_ATUALIZACAO_RENDA DATE,LINHA_ARQUIVO INTEGER NOT NULL,CONSTRAINT SYS_FK_63 FOREIGN KEY(CODIGO_INVESTIGADO) REFERENCES INVESTIGADO(CODIGO_INVESTIGADO))
CREATE INDEX IDX_PESSOA ON PESSOA(TIPO_PESSOA,CPF_CNPJ_PESSOA,NOME_PESSOA,NOME_DOC_IDENTIFICACAO,NUMERO_DOC_IDENTIFICACAO)
CREATE INDEX IDX_PESSOA_LINHA ON PESSOA(LINHA_ARQUIVO)
CREATE CACHED TABLE TITULARIDADE(CODIGO_CHAVE_PESSOA BIGINT NOT NULL,CODIGO_CHAVE_CONTA BIGINT NOT NULL,TIPO_TITULAR VARCHAR(1),PESSOA_INVESTIGADA NUMERIC(1) DEFAULT 0,DATA_INICIO_RELACIONAMENTO DATE NOT NULL,DATA_FIM_RELACIONAMENTO DATE,LINHA_ARQUIVO INTEGER NOT NULL,PRIMARY KEY(CODIGO_CHAVE_PESSOA,CODIGO_CHAVE_CONTA),CONSTRAINT SYS_FK_68 FOREIGN KEY(CODIGO_CHAVE_PESSOA) REFERENCES PESSOA(CODIGO_CHAVE_PESSOA) ON DELETE CASCADE ON UPDATE CASCADE,CONSTRAINT SYS_FK_69 FOREIGN KEY(CODIGO_CHAVE_CONTA) REFERENCES CONTA(CODIGO_CHAVE_CONTA) ON DELETE CASCADE ON UPDATE CASCADE)
CREATE CACHED TABLE ORIGEM_DESTINO(CODIGO_CHAVE_OD NUMERIC(20) NOT NULL PRIMARY KEY,CODIGO_CHAVE_EXTRATO BIGINT NOT NULL,VALOR_OPERACAO NUMERIC(14) NOT NULL,NUMERO_DOCUMENTO VARCHAR(20),NUMERO_BANCO_OD BIGINT,NUMERO_AGENCIA_OD NUMERIC(4),NUMERO_CONTA_OD VARCHAR(20),TIPO_CONTA_OD NUMERIC(1),TIPO_PESSOA_OD NUMERIC(1),CPF_CNPJ_PESSOA_OD VARCHAR(14),NOME_PESSOA_OD VARCHAR(80),NOME_DOC_IDENTIFICACAO_OD VARCHAR(50),NUMERO_DOC_IDENTIFICACAO_OD VARCHAR(20),CODIGO_DE_BARRAS VARCHAR(100),NOME_ENDOSSANTE_CHEQUE VARCHAR(80),DOC_ENDOSSANTE_CHEQUE VARCHAR(50),SITUACAO_IDENTIFICACAO NUMERIC(1),OBSERVACAO_OD VARCHAR(250),LINHA_ARQUIVO INTEGER NOT NULL,CONSTRAINT SYS_FK_76 FOREIGN KEY(CODIGO_CHAVE_EXTRATO) REFERENCES EXTRATO(CODIGO_CHAVE_EXTRATO) ON DELETE CASCADE ON UPDATE CASCADE)
