CREATE CACHED TABLE GAB800VER2(CODIGO_CHAVE_GAB800VER2 BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,CODIGO_DO_CLIENTE NUMERIC(9),NOME_DO_CLIENTE VARCHAR(60),ENDERECO VARCHAR(52),ENDERECO_COMPLEMENTO VARCHAR(47),DATA_DE_REFERENCIA DATE,DATA_DE_LIQUIDACAO DATE,DESCRICAO_DO_LANCAMENTO VARCHAR(80),SALDO_INICIAL NUMERIC(17),NATUREZA_DO_SALDO_INICIAL VARCHAR(1),VALOR_DO_LANCAMENTO NUMERIC(17),NATUREZA_DO_LANCAMENTO VARCHAR(1),CHAVE_OD NUMERIC(20),LINHA_ARQUIVO INTEGER)
